{
    "version": "0.1.0",
    "platform": {
        "os": "linux",
        "arch": "amd64"
    },
    "process": {
        "terminal": true,
        "user": {
            "uid": 0,
            "gid": 0,
            "additionalGids": null
        },
        "args": [
            "/bin/bash"
        ],
        "env": [
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "TERM=xterm"
        ],
        "cwd": ""
    },
    "root": {
        "path": "rootfs",
        "readonly": true
    },
    "hostname": "shell",
    "mounts": [
        {
            "name": "proc",
            "path": "/proc"
        },
        {
            "name": "dev",
            "path": "/dev"
        },
        {
            "name": "devpts",
            "path": "/dev/pts"
        },
        {
            "name": "shm",
            "path": "/dev/shm"
        },
        {
            "name": "mqueue",
            "path": "/dev/mqueue"
        },
        {
            "name": "sysfs",
            "path": "/sys"
        },
        {
            "name": "cgroup",
            "path": "/sys/fs/cgroup"
        }
    ],
    "linux": {
        "capabilities": [
            "CAP_AUDIT_WRITE",
            "CAP_KILL",
            "CAP_NET_BIND_SERVICE"
        ]
    }
}
